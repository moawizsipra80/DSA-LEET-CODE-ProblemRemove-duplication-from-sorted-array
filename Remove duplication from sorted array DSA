class Solution {
public:
    int removeDuplicates(vector<int>& nums) 
    {
    // sort(nums.begin(),nums.end());
    int count=1;
    int n=nums.size();
    int lp=1;
    int rp=n-1;
    while(lp<=rp)
    {
           if(nums[lp]!=nums[lp-1])
           {
            nums[count]=nums[lp];
            count++;
           }
      
                       lp++;
    }
    return count;   
    }
};
